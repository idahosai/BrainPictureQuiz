<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.iidah.brainpicturequiz.PicturePageActivity">

    <ImageView
        android:id="@+id/img_two"
        android:layout_width="95dp"
        android:layout_height="117dp"
        android:layout_marginStart="144dp"
        android:layout_marginTop="52dp"
        android:src="@mipmap/ic_launcher"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/img_three"
        android:layout_width="95dp"
        android:layout_height="118dp"
        android:layout_marginEnd="24dp"
        android:layout_marginTop="52dp"
        android:src="@mipmap/ic_launcher"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/img_four"
        android:layout_width="95dp"
        android:layout_height="118dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="28dp"
        android:src="@mipmap/ic_launcher"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_one" />

    <ImageView
        android:id="@+id/img_five"
        android:layout_width="95dp"
        android:layout_height="118dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="28dp"
        android:src="@mipmap/ic_launcher"
        app:layout_constraintStart_toEndOf="@+id/img_four"
        app:layout_constraintTop_toBottomOf="@+id/img_two" />

    <ImageView
        android:id="@+id/img_six"
        android:layout_width="95dp"
        android:layout_height="118dp"
        android:layout_marginEnd="24dp"
        android:layout_marginTop="28dp"
        android:src="@mipmap/ic_launcher"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_three" />

    <ImageView
        android:id="@+id/img_one"
        android:layout_width="95dp"
        android:layout_height="118dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="52dp"
        android:src="@mipmap/ic_launcher"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/img_eight"
        android:layout_width="95dp"
        android:layout_height="118dp"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="28dp"
        android:src="@mipmap/ic_launcher"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/img_seven"
        app:layout_constraintTop_toBottomOf="@+id/img_five"
        app:layout_constraintVertical_bias="0.02" />

    <ImageView
        android:id="@+id/img_nine"
        android:layout_width="95dp"
        android:layout_height="118dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="28dp"
        android:layout_marginTop="28dp"
        android:src="@mipmap/ic_launcher"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_six"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/img_seven"
        android:layout_width="95dp"
        android:layout_height="118dp"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="28dp"
        android:src="@mipmap/ic_launcher"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_four"
        app:layout_constraintVertical_bias="0.0" />
</android.support.constraint.ConstraintLayout>
